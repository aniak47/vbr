<h1>Staffs#new</h1>
<% provide(:title, 'Sign up') %>
<h1>Sign up</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
       <%= form_for(@staff) do |f| %>
       <%= render 'shared/error_messages', object: f.object  %>

      <%= f.label :email %>
      <%= f.email_field :email, class: 'form-control' %>

      <%= hidden_field_tag :password, "bb39fn3o4bfo2euofboub" %>

      <%= hidden_field_tag :password_confirmation, "bb39fn3o4bfo2euofboub" %>

      <%= f.submit "Create User", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
